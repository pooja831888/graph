int [][]matrix=new int[V][V];

// for logic
for(int i=0; i<edges.length; i++)
{
    int u=edges[i][0];
    int v=edges[i][1];
    matix[u][v]=1;
    matix[v][u]=1;
}


//for print
for(int i=0; i<v; i++)
{
    for(int j=0; j<v; j++)
    {
        System.out.println("matrix repsrentation"+matix[i][j]);
    }
}
